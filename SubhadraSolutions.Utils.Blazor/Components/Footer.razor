@inherits AbstractSmartComponent<MudPaper Height="100px" Elevation="0" Class="py-1"></MudPaper><MudAppBar Bottom="true" Fixed="true" Dense="true" Gutters="false">	<MudPaper Outlined="@this.Outlined" Elevation="@this.Elevation" Square="@this.Square" Class="@this.GetClass()" Style="@this.GetStyle()" Tag="@this.Tag" UserAttributes="@this.UserAttributes">		<MudGrid Spacing="1">			<MudItem md="4">				<MudText Typo="Typo.body2">@this.ProductInfo.Name. Version: @this.ProductInfo.Version.</MudText>			</MudItem>			<MudItem md="4">				<MudText Typo="Typo.body2">Copyright Â© @DateTime.Now.Year <MudLink Typo="Typo.body2" Href="@this.CompanyInfo.CompanyUrl" Target="_blank">@this.CompanyInfo.Name.</MudLink></MudText>			</MudItem>			<MudItem md="4">				@if (this.ProductInfo.ForInternalUseOnly)				{
					var temp = CompanyInfo.ShortName + (this.CompanyInfo.ShortName.EndsWith("'s") ? null : "'s");					<MudText Typo="Typo.body2">For @temp internal-use only</MudText>
				}			</MudItem>			<MudItem md="12">				<MudText Typo="Typo.body2"> For more information on how @this.ProductInfo.Name and other @this.CompanyInfo.ShortName services use the personal information of @this.CompanyInfo.ShortName employees and external staff, see the  <MudLink Typo="Typo.body2" Href="@this.CompanyInfo.PrivacyStatementUrl" Target="_blank">@this.CompanyInfo.ShortName Global Data Protection Notice for Employees, External Staff and Candidates.</MudLink></MudText>			</MudItem>		</MudGrid>	</MudPaper></MudAppBar>