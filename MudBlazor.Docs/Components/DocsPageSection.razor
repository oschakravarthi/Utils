<CascadingValue Value="this" IsFixed="true">
    @*<QueuedContent RenderImmediately="_renderImmediately">
    <div @attributes="UserAttributes" class="docs-page-section">
    @ChildContent
    </div>
    </QueuedContent>*@
    @if (this.DocsPage != null && this.DocsPage.PrintView)
    {
        <div @attributes="UserAttributes" class="docs-page-section" style="@this.PrintViewStyle">
            @ChildContent
        </div>
    }
    else
    {
        <div @attributes="UserAttributes" class="docs-page-section">
            @ChildContent
        </div>
    }
    </CascadingValue>